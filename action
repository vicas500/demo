public class Main {
  public static void main(String[] args) {
    String originalStr = "brundha";
    String reversedStr = "";
    System.out.println("monika: " + originalStr);
        
    for (int i = 0; i < originalStr.length(); i++) {
      reversedStr = originalStr.charAt(i) + reversedStr;
    }
    
    System.out.println("praga: "+ reversedStr);
  }
}

