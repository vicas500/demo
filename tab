public class Main {
  public static void main(String[] args) {
    int day = 4;
    switch (day) {
      case 1:
        System.out.println("string");
        break;
      case 2:
        System.out.println("berak");
        break;
      case 3:
        System.out.println("void");
        break;
      case 4:
        System.out.println("case");
        break;
      case 5:
        System.out.println("run");
        break;
      case 6:
        System.out.println("out");
        break;
      case 7:
        System.out.println("system");
        break;
    }
  }
}
